<div id="app">
    <table>
        <thead>
        <tr>
            <th></th>
            <th>Description</th>
            <th colspan="2"></th>
        </tr>
        </thead>
        <tbody>
        <tr v-for="todo in todos">
            <td>
                <input type="checkbox" disabled :checked="todo.complete">
            </td>
            <td>{{ todo.description }}</td>
            <td>
                <button @@click="onEdit(todo.id)">Edit</button>
            </td>
            <td>
                <button @@click="onDelete(todo.id)">Delete</button>
            </td>
        </tr>
        </tbody>
    </table>
    <button @@click="onAdd">Add</button>
</div>

<script src="/build/runtime.js"></script>
<script src="/build/vendors-node_modules_vue_dist_vue_esm-bundler_js-node_modules_axios_lib_axios_js.js"></script>
<script src="/build/home/index.js"></script>
